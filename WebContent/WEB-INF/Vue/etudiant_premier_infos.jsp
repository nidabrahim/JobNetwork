        <div class="registration-container">            
            <div class="registration-box animated fadeInDown" style="width: 85%" >
                
                
                
                <div class="panel panel-default">
                                <div class="panel-body">
                                    <h3>Completer votre inscription</h3>                                
                                    <form action="/jobNetwork/updateInfos" role="form" class="form-horizontal" id="wizard-validation" method="POST" >
                                    <div class="wizard show-submit wizard-validation">
                                        <ul>
                                            <li>
                                                <a href="#step-7">
                                                    <span class="stepNumber">1</span>
                                                    <span class="stepDesc">Login<br /><small>Information</small></span>
                                                </a>
                                            </li>
                                            <li>
                                                <a href="#step-8">
                                                    <span class="stepNumber">2</span>
                                                    <span class="stepDesc">Uilisateur<br /><small>Donnees Personelles</small></span>
                                                </a>
                                            </li>                                    
                                        </ul>

                                        <div id="step-7">   

                                            <div class="form-group">
                                                <label class="col-md-2 control-label">Login</label>
                                                <div class="col-md-10">
                                                    <input type="text" class="form-control" name="email" placeholder="Email"/>
                                                </div>
                                            </div>
                                            <div class="form-group">
                                                <label class="col-md-2 control-label">Mot de passe</label>
                                                <div class="col-md-10">
                                                    <input type="password" class="form-control" name="password" placeholder="Mot de passe" id="password"/>
                                                </div>
                                            </div>             
                                            <div class="form-group">
                                                <label class="col-md-2 control-label">Confirme</label>
                                                <div class="col-md-10">
                                                    <input type="password" class="form-control" name="repassword" placeholder="Confirmation mot de passe"/>
                                                </div>
                                            </div>

                                        </div>

                                        <div id="step-8">

                                            <div class="form-group">
                                                <label class="col-md-2 control-label">Date naissance</label>
                                                <div class="col-md-10">
                                                    <input type="text" class="mask_date form-control" name="dateNaissance" placeholder="Date de naissance"/>
                                                </div>
                                            </div>
                                            
                                            <div class="form-group">
                                                <label class="col-md-2 control-label">Tel </label>
                                                <div class="col-md-10">
                                                    <input type="text" class="form-control" name="telFixe" placeholder="Tel fixe"  style="width:49%;float:left"  />
                                                     <input type="text" class="form-control" name="telGsm" placeholder="Tel GSM" style="width:49%;float:right" />
                                                </div>
                                            </div>                

											<div class="form-group">
                                                <label class="col-md-2 control-label"> </label>
                                                <div class="col-md-10">
                                                    <input type="text" class="form-control" name="cp" placeholder="Code postal"  style="width:49%;float:left"/>
                                                     <input type="text" class="form-control" name="ville" placeholder="Ville" style="width:50%;float:right" />
                                                </div>
                                            </div>   
                                           
                                            
                                            <div class="form-group">
                                                <label class="col-md-2 control-label">Adresse</label>
                                                <div class="col-md-10">
                                                    <input type="text" class="form-control" name="adresse" placeholder="Adresse"/>
                                                </div>
                                            </div> 
                                            
                                                

                                        </div>               
                                                                                                                                     
                                    </div>
                                    
                                    </form>
                                </div>
